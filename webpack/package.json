{
  "name": "webpack-module",
  "version": "1.0.0",
  "description": "Webpack module",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config=dev.webpack.config.js --open",
    "compile": "rimraf dist && webpack --config=prod.webpack.config.js",
    "compile:pre": "rimraf dist && webpack --config=dev.webpack.config.js",
    "serve:pre": "rimraf dist && webpack --config=dev.webpack.config.js && serve -s dist",
    "serve:pro": "rimraf dist && webpack --config=prod.webpack.config.js && serve -s dist"
  },
  "author": "Álvaro Gómez Jáuregui",
  "dependencies": {
    "@babel/polyfill": "7.2.5",
