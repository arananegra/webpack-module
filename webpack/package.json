    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.5",
    "rimraf": "2.6.3",
    "@types/node": "12.0.0",
    "@types/core-js": "0.9.46",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "@types/webpack-env": "1.13.3",
