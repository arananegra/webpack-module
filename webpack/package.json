{
  "name": "webpack-module",
  "version": "1.0.0",
  "description": "Webpack module",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=pre-compilation --hot --inline --devtool=inline-source-map --progress",
    "start:devtool-none": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=pre-compilation --hot --inline --devtool=none --progress",
    "compile": "rimraf dist && ./node_modules/webpack-cli/bin/cli.js --config=pro-compilation",
    "compile:pre": "rimraf dist && ./node_modules/webpack-cli/bin/cli.js --config=pre-compilation",
    "serve:pre": "rimraf dist && ./node_modules/webpack-cli/bin/cli.js --config=pre-compilation && ./node_modules/serve/bin/serve.js -s dist",
    "serve:pro": "rimraf dist && ./node_modules/webpack-cli/bin/cli.js --config=pro-compilation && ./node_modules/serve/bin/serve.js -s dist"
  },
  "author": "Álvaro Gómez Jáuregui",
  "dependencies": {
    "bootstrap": "4.3.1",
    "core-js": "2.5.3",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "serve": "10.0.2"
  },
  "devDependencies": {
    "rimraf": "2.6.3",
    "@types/core-js": "0.9.46",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "@types/webpack-env": "1.13.3",
    "css-loader": "0.28.11",
    "style-loader": "0.23.1",
    "sass-loader": "7.1.0",
    "sass": "1.20.1",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.6.0",
    "ts-loader": "6.0.0",
    "typescript": "3.4.5",
    "url-loader": "1.1.2",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1",
    "webpack-pwa-manifest": "4.0.0"
  }
}
